{
  "name": "burstjs",
  "version": "1.0.0",
  "description": "Common BURST library",
  "contributors": [
    "ohager",
    "blankey"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/burst-apps-team/phoenix"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "GPL-3.0",
  "private": false,
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.1.0",
    "custom-tslint-formatters": "^2.4.0",
    "esdoc": "^1.1.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lerna": "^3.10.5",
    "regenerator-runtime": "^0.13.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage && codecov",
    "test:e2e": "jest --config jest.config.e2e.js --runInBand --detectOpenHandles --forceExit",
    "doc": "./node_modules/.bin/esdoc",
    "bootstrap": "lerna bootstrap && lerna exec npm install",
    "lint": "tslint -p tsconfig.json -t stylish -s node_modules/custom-tslint-formatters/formatters -t grouped",
    "publish": "lerna run tsc && lerna publish"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
