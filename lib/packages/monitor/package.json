{
  "name": "@burstjs/monitor",
  "version": "0.6.0-beta.5",
  "description": "Monitor transactions on Burst blockchain",
  "contributors": [
    "ohager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/burst-apps-team/phoenix"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "Apache-2.0",
  "main": "./out/index.js",
  "typings": "./out/index.d.ts",
  "private": false,
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "bili": "^4.8.1",
    "node-localstorage": "^2.1.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.2"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "bundle": "bili",
    "readme": "jsdoc2md  --files ./src/*.ts --template ./README.md.hbs --configure ../../jsdoc2md.json > ./README.md"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1b375dedd063baa0933556cc50f87b5c926ba12d"
}
