{
  "name": "@burstjs/core",
  "version": "0.5.1",
  "description": "Burst-related functions and models for building Burstcoin applications.",
  "contributors": [
    "ohager",
    "blankey1337"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/burst-apps-team/phoenix"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "Apache-2.0",
  "main": "./out/index.js",
  "typings": "./out/index.d.ts",
  "private": false,
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "bili": "^4.8.1",
    "dotenv": "^8.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.2"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "bundle": "bili",
    "readme": "jsdoc2md  --files ./src/**/*.ts --template ./README.md.hbs --configure ../../jsdoc2md.json > ./README.md"
  },
  "dependencies": {
    "@burstjs/contracts": "^0.5.1",
    "@burstjs/crypto": "^0.5.1",
    "@burstjs/http": "^0.5.1",
    "@burstjs/util": "^0.5.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "gitHead": "9b5a4e07616d783e85bcae6c21b72f50db783f48",
  "publishConfig": {
    "access": "public"
  }
}
