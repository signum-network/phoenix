{
  "name": "@burstjs/core",
  "version": "0.0.11",
  "description": "Burst-related functions and models for building Burstcoin applications.",
  "contributors": [
    "ohager",
    "blankey1337"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/burst-apps-team/phoenix"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "GPL-3.0",
  "main": "./out/index.js",
  "typings": "./out/index.d.ts",
  "private": false,
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "dotenv": "^6.2.0"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "doc": "./node_modules/.bin/esdoc",
    "readme": "jsdoc2md  --files ./src/*.ts --template ./README.md.hbs --configure ../../jsdoc2md.json > ./README.md"
  },
  "dependencies": {
    "@burstjs/crypto": "^0.0.11",
    "@burstjs/http": "^0.0.11",
    "bn.js": "^4.11.8"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "gitHead": "d66916118ee787da2aacfc18b9809c66617c2b57",
  "publishConfig": {
    "access": "public"
  }
}
