{
  "name": "burstjs - The Burstcoin Reference Library",
  "mode": "modules",
  "hideGenerator": true,
  "inputFiles": ["./packages/**/*.ts"],
  "out": "../docs",
  "theme": "docstheme/default",
  "excludeExternals": true,
  "excludeNotExported": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "stripInternal": true,
  "readme": "DOCINDEX.md",
  "exclude": [
    "/node_modules/",
    "/__tests__/",
    "/dist/",
    "README.md"
  ]
}

