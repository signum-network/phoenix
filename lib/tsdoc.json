{
  "name": "burstjs - The Burstcoin Reference Library",
  "mode": "modules",
  "inputFiles": ["./packages"],
  "out": "../docs",
  "theme": "docstheme/default",
  "excludeNotExported": true,
  "external-modulemap": ".*packages\/.*",
  "excludePrivate": true,
  "readme": "DOCINDEX.md",
  "exclude": [
    "/node_modules/",
    "/__tests__/",
    "/dist/",
    "README.md"
  ]
}
