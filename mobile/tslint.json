{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": true,
    "object-literal-sort-keys": false,
    "member-access": false,
    "member-ordering": false,
    "jsx-no-lambda": true,
    "jsx-boolean-value": false
  },
  "rulesDirectory": []
}