{
  "name": "phoenix-mobile-wallet",
  "description": "The mobile wallet for Signum",
  "version": "1.0.0",
  "keywords": [
    "cryptocoin",
    "signum",
    "blockchain",
    "mobile",
    "wallet"
  ],
  "contributors": [
    "titanishu",
    "blankey1337",
    "ohager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signum-network/phoenix"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "check": "npm run check:lint && npm run check:tsc",
    "check:lint": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}'",
    "check:tsc": "tsc --noEmit",
    "dev": "npm run dev:ios",
    "dev:ios": "react-native run-ios --simulator=\"iPhone SE\"",
    "start:android": "react-native run-android",
    "start:metro": "react-native start",
    "start": "IS_RELEASE=false && concurrently npm:start:*",
    "postinstall": "npm run link",
    "link": "react-native link",
    "translations:sort": "node tasks/sortJsonDir.js src/translations && git add src/translations/*.json",
    "test": "jest",
    "tsc": "tsc --init --pretty --jsx react",
    "prepare": "husky install",
    "lint": "lint-staged"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/checkbox": "^0.5.7",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/push-notification-ios": "^1.0.3",
    "@react-native-community/slider": "^2.0.0",
    "@react-navigation/bottom-tabs": "^5.7.3",
    "@react-navigation/native": "^5.7.2",
    "@react-navigation/stack": "^5.8.0",
    "@signumjs/core": "^1.0.0-rc.3",
    "@signumjs/crypto": "^1.0.0-rc.3",
    "@signumjs/http": "^1.0.0-rc.3",
    "@signumjs/util": "^1.0.0-rc.3",
    "css-tree": "1.0.0-alpha.39",
    "d3-shape": "^1.3.5",
    "i18n-js": "^3.2.2",
    "lodash": "^4.17.21",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-camera": "^3.35.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-keychain": "^6.1.1",
    "react-native-localize": "^1.4.1",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-navigation": "^6.10.1",
    "react-native-picker-select": "^7.0.0",
    "react-native-push-notification": "^4.0.0",
    "react-native-qrcode-scanner": "^1.4.1",
    "react-native-qrcode-svg": "^6.1.1",
    "react-native-reanimated": "^1.10.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-svg": "^12.1.1",
    "react-native-svg-charts": "^5.4.0",
    "react-native-swipeout": "^2.3.6",
    "react-native-touch-id": "^4.4.1",
    "react-native-version-number": "^0.3.6",
    "react-navigation": "^4.4.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "rn-swipe-button": "^1.2.9"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/runtime": "^7.11.0",
    "@types/d3-shape": "^1.3.1",
    "@types/i18n-js": "^3.0.1",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.129",
    "@types/react": "^16.8.17",
    "@types/react-native": "^0.57.57",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-svg-charts": "^5.0.2",
    "@types/react-native-touch-id": "^4.0.1",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-navigation": "^3.0.7",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^16.8.1",
    "babel-jest": "^27.0.6",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^6.2.0",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "metro-react-native-babel-preset": "0.54.1",
    "prettier": "2.4.1",
    "react-devtools": "^4.10.4",
    "react-native-cli": "^2.0.1",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.8.3",
    "sort-json": "^2.0.0",
    "ts-jest": "^27.0.3",
    "typescript": "^3.9.10"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
